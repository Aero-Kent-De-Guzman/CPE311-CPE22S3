      "source": [
        "class PassengerToShore():\n",
        "    def __init__(self, contents):\n",
        "        self.contents = contents\n",
        "\n",
        "    def dict_sort(self):\n",
        "        temp = list(self.contents.items())\n",
        "        for i in range(len(temp)):\n",
        "            for j in range(len(temp) - 1 - i):\n",
        "                if temp[j][1] > temp[j + 1][1]:\n",
        "                    temp[j], temp[j + 1] = temp[j + 1], temp[j]\n",
        "        self.contents = dict(temp)\n",
        "\n",
        "    def BoatCross(self):\n",
        "        island = list(self.contents.items())\n",
        "        boat = []\n",
        "        shore = []\n",
        "        temp = []\n",
        "        boatKG = 0\n",
        "        islandPosition = 0\n",
        "        shorePosition = 0\n",
        "\n",
        "        print(\"##############################################################\")\n",
        "        print(\"\\t  EVENT 1 : CARRYING SUPPLIES FIRST TO SHORE\")\n",
        "        print(\"##############################################################\\n\")\n",
        "        print(\"current status : \\nisland\\t\",island,\"\\nboat\\t\",boat,\"\\nshore\\t\",shore,\"\\n\")\n",
        "        # Event 2 : Carry first the supplies\n",
        "        for key, val in island:\n",
        "            if key == \"Supplies\":\n",
        "                islandPosition = 0\n",
        "                boat.append((key, val))\n",
        "                island.pop(islandPosition)\n",
        "                temp += island\n",
        "                print(\"Supplies have boarded the boat\\n\")\n",
        "                print(\"current status : \\nisland\\t\",island,\"\\nboat\\t\",boat,\"\\nshore\\t\",shore,\"\\n\")\n",
        "                for key, val in island:\n",
        "                    if (val + boat[0][1]) < 100:\n",
        "                        boat.append(island[islandPosition])\n",
        "                        island.pop(islandPosition)\n",
        "                        print(\"Person have boarded the boat\\n\")\n",
        "                        print(\"current status : \\nisland\\t\",island,\"\\nboat\\t\",boat,\"\\nshore\\t\",shore,\"\\n\")\n",
        "                        islandPosition = 0\n",
        "                        shore += boat\n",
        "                        temp.clear()\n",
        "                        temp += shore\n",
        "                        boat.clear()\n",
        "                        print(\"All were dropped off to shore\\n\")\n",
        "                        print(\"current status : \\nisland\\t\",island,\"\\nboat\\t\",boat,\"\\nshore\\t\",shore,\"\\n\")\n",
        "                        boat.append(shore[1])\n",
        "                        shore.pop(1)\n",
        "                        print(\"Driver returning to island\\n\")\n",
        "                        print(\"current status : \\nisland\\t\",island,\"\\nboat\\t\",boat,\"\\nshore\\t\",shore,\"\\n\")\n",
        "                        island = boat + island # do not change the placement, it will destroy the arrangement\n",
        "                        boat.clear()\n",
        "                        temp.clear()\n",
        "                        print(\"current status : \\nisland\\t\",island,\"\\nboat\\t\",boat,\"\\nshore\\t\",shore,\"\\n\")\n",
        "                        break\n",
        "                    islandPosition += 1\n",
        "            islandPosition += 1\n",
        "\n",
        "        print(\"##############################################################\")\n",
        "        print(\" EVENT 2 : Transfering the people in the island to the shore.\")\n",
        "        print(\"##############################################################\\n\")\n",
        "        # Event 4 : Delivering all passengers to shore\n",
        "        print(\"current status : \\nisland\\t\",island,\"\\nboat\\t\",boat,\"\\nshore\\t\",shore,\"\\n\")\n",
        "        #self.dict_sort()\n",
        "        while(len(island) != 0):\n",
        "            # Going to Shore\n",
        "            print(\"__________\\n\\nCURRENTLY ON THE ISLAND\\n\\nPassengers are boarding...\\n\")\n",
        "            temp = []\n",
        "            for key, val in island[:]:\n",
        "                boatKG += val\n",
        "                if boatKG <= 100:\n",
        "                    boat.append((key,val))\n",
        "                    temp = island.pop(0)\n",
        "                    print(\"current status : \\nisland\\t\",island,\"\\nboat\\t\",boat,\"\\nshore\\t\",shore,\"\\nboatKG\\t\",boatKG,'\\n')\n",
        "                else:\n",
        "                    shore += boat\n",
        "                    boat.clear()\n",
        "                    boatKG = 0\n",
        "                    print(\"Max capacity! going to shore while returning exceed to original position...\\n\")\n",
        "                    print(\"current status : \\nisland\\t\",island,\"\\nboat\\t\",boat,\"\\nshore\\t\",shore,\"\\nboatKG\\t\",boatKG,'\\n')\n",
        "                    break\n",
        "\n",
        "            if len(island) == 0:\n",
        "                shore += boat\n",
        "                boat.clear()\n",
        "                break\n",
        "\n",
        "            # Going to Island\n",
        "            print(\"__________\\n\\nCURRENTLY ON THE SHORE\\n\")\n",
        "            shorePosition = 0\n",
        "            for key, val in shore:\n",
        "                if key == 'Supplies': # will ignore the supplies, which is why do not change the var name please...\n",
        "                    shorePosition += 1\n",
        "                    continue\n",
        "                boat.append((key, val))\n",
        "                shore.pop(shorePosition)\n",
        "                print(\"Driver from shore is entering the boat.\\n\")\n",
        "                print(\"current status : \\nisland\\t\",island,\"\\nboat\\t\",boat,\"\\nshore\\t\",shore,\"\\n\")\n",
        "                island += boat\n",
        "                boat.clear()\n",
        "                print(\"All people in the boat are dropping off.\\n\")\n",
        "                print(\"current status : \\nisland\\t\",island,\"\\nboat\\t\",boat,\"\\nshore\\t\",shore,\"\\n\")\n",
        "                break\n",
        "\n",
        "        print(\"__________\\n\\nSUCCESSFULLY TRANSFERRED EVERYONE\")\n",
        "        print(\"current status : \\nisland\\t\",island,\"\\nboat\\t\",boat,\"\\nshore\\t\",shore,\"\\n\")\n",
        "\n",
        "Passengers = {\n",
        "    'Roman' : 90,\n",
        "    'Verlyn' : 80,\n",
        "    'Supplies' : 20, # Please DO NOT change the name of the variable, the name is the identifier which should not be handled later on...\n",
        "    'Lloyd' : 60,\n",
        "    'Robin' : 40,\n",
        "    }\n",
        "\n",
        "Boat = PassengerToShore(Passengers)\n",
        "Boat.dict_sort()\n",
        "Boat.BoatCross()"
      ]
    }
